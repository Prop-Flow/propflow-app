{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "propflow-app",
          "region": "us-east4"
        }
      }
    ]
  },
  "firestore": {
    "indexes": "firestore.indexes.json"
  },
  "extensions": {
    "firestore-genai-chatbot": "googlecloud/firestore-genai-chatbot@0.0.17",
    "firestore-multimodal-genai": "googlecloud/firestore-multimodal-genai@1.0.3"
  }
}